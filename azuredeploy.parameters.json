{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"apiAppName": {
		"type": "string",
		},
		"databaseAccountName": {
			"type": "string",
			"defaultValue": "databaseAccountName"
		},
		"dataBase": {
			"type": "string",
			"defaultValue": "dataBaseName",
		},
		"appSvcPlanName": {
			"type": "string",
			"defaultValue": "appSvcPlanName"
		},
			"svcPlanSize": {
			"type": "string",
			"defaultValue": "S1",

		},
			"svcPlanSku": {
			"type": "string",
			"allowedValues": [
				"Free",
				"Shared",
				"Basic",
				"Standard",
				"Premium"
			],
			"defaultValue": "Standard",
		}
	}
}
