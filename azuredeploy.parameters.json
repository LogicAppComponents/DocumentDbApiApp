{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"webAppName": {
		"type": "string",
		},
		"databaseAccountName": {
			"type": "string",
			"defaultValue": "commonlogic-test"
		},
		"dataBase": {
			"type": "string",
			"defaultValue": "data_entity_catalog",
		},
		"appSvcPlanName": {
			"type": "string",
			"defaultValue": "CommonLogicTestAppPlan01"
		},
			"svcPlanSize": {
			"type": "string",
			"defaultValue": "S1",

		},
			"svcPlanSku": {
			"type": "string",
			"allowedValues": [
				"Free",
				"Shared",
				"Basic",
				"Standard",
				"Premium"
			],
			"defaultValue": "Standard",
		},
			"gatewayName": {
      			"value": "ExistingGateway"
    		},
    			"apiAppSecret": {
      			"value": "NotSecureSecret"
		 },
		 "location": {
      "value": "West US"
    },
     "hostingPlanId": {
      "value": "ExistingID"
    }
	}
}
